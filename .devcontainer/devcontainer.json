{
    "name": "multimatics-dev",
    "service": "multimatics-devel",
    "dockerComposeFile": "../docker-compose.devel.yaml",
    // "features": {
    //     "ghcr.io/devcontainers/features/docker-outside-of-docker": {}
    // },
    "workspaceFolder": "/home/node/workspace",
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/node/workspace,type=bind",
    "containerEnv": {
        "TZ": "Asia/Jakarta"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.vscode-typescript-next",
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint",
                "bradlc.vscode-tailwindcss",
                "astro-build.astro-vscode",
                "expo.vscode-expo-tools",
                "msjsdiag.vscode-react-native",
                "Prisma.prisma",
                "ritwickdey.LiveServer",
                "ms-vscode.makefile-tools",
                "eamodio.gitlens",
                "redhat.vscode-yaml",
                "yzhang.markdown-all-in-one"
            ]
        }
    }
}